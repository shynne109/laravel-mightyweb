{
    "name": "shynne109/mightyweb",
    "version": "1.1.0",
    "description": "Laravel package for mobile app configuration management with Livewire Volt, Flux, and Tailwind CSS",
    "keywords": [
        "laravel",
        "livewire",
        "mobile-app",
        "app-configuration",
        "tailwind",
        "alpine"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adedoyin Shina",
            "email": "sbatch2016@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2|^8.3",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0",
        "livewire/livewire": "^3.5",
        "livewire/flux": "^2.6",
        "intervention/image": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "phpunit/phpunit": "^10.5|^11.0"
    },
    "autoload": {
        "psr-4": {
            "MightyWeb\\": "src/",
            "MightyWeb\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MightyWeb\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MightyWeb\\MightyWebServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
